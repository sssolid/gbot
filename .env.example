# File: .env.example
# Location: /.env.example
# Copy this file to .env and fill in your values

# Discord Bot Token (required)
# Get this from https://discord.com/developers/applications
DISCORD_TOKEN=your_bot_token_here

# Command Prefix (optional, default: !)
COMMAND_PREFIX=!

# Database URL
# SQLite (default): sqlite:///bot.db
# PostgreSQL: postgresql://username:password@localhost:5432/dbname
DATABASE_URL=sqlite:///bot.db

# Logging Level (optional, default: INFO)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Debug Mode (optional, default: False)
# Set to True for verbose SQL logging
DEBUG_MODE=False